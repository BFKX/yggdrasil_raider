<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.text.Text?>

<AnchorPane fx:id="myPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.StartMenuController">
   <children>
      <Button fx:id="playButton" mnemonicParsing="false" text="Play" />

      <Button fx:id="quitButton" mnemonicParsing="false" text="Quit" />

      <Text fx:id="myText" />

      <Text fx:id="title" />
   </children>
</AnchorPane>
